{
  "name": "dell-email-builder",
  "id": "dell-email-builder",
  "version": "1.0.0",
  "main": "index.html",
  "manifestVersion": 5,
  "requiredPermissions": {
    "allowCodeGenerationFromStrings": true,
    "localFileSystem": "fullAccess"
  },
  "featureFlags": {
    "enableSWCSupport": true
  },
  "permissions": {
    "localFileSystem": [
      "read",
      "write"
    ],
    "launchProcess": {
      "schemes": [
        "https",
        "slack",
        "adbpsd"
      ],
      "extensions": [
        ".pdf",
        ".xd",
        ".psd",
        ".csv",
        ".jpg"
      ]
    }
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "vanilla",
      "label": {
        "default": "Dell Email Builder"
      },
      "minimumSize": {
        "width": 230,
        "height": 200
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 230,
        "height": 300
      },
      "preferredFloatingSize": {
        "width": 230,
        "height": 300
      }
    }
  ],
  "host": [
    {
      "app": "PS",
      "minVersion": "24.4.0"
    }
  ]
}